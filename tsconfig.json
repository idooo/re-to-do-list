{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"experimentalDecorators": true,
		"baseUrl": ".",
		"paths": {
			"*": [
				"node_modules/*",
				"src/types/*"
			]
		}
	},
	"include": [
		"./src/**/*",
		"./server.ts"
	]
}
